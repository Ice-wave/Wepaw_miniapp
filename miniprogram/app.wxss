/**app.wxss**/

@import "color.wxss";
@import "./components/iconfont/iconfont.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

page{
	height: 100%;
	width: 100%;
	display: flex;
	flex-direction: column;
}


.page-content {
	flex: 1;
	height: 0;
}



.line {
  height: 1px;
  background-color: #f8f8f8;
  margin: 18.72rpx 0;
}

.horizontal {
  display: flex;
  align-items: center;
}


